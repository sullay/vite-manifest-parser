    "compilerOptions": {
        "target": "ESNext",
        "module": "ESNext",
        "strict": true,
        "noImplicitAny": false,
        "esModuleInterop": true,
        "moduleResolution": "Node",
        "declaration": true,
        "skipLibCheck": true
